package Main;

import java.util.ArrayList;

public class MainTestClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] arrayInteger = { 123, 2345, 345};
        // arrayString = { 123, 2345, 345};
		
		System.out.println(convertArrayToString(arrayInteger));
		
	}

	public static String [] convertArrayToString(int[] arrayInt) {
		ArrayList <String> arrayStr = new ArrayList<>();
		for(int i = 0; i < arrayInt.length; i++) {
			String element = "";
			element = String.valueOf(arrayInt[i]);
			arrayStr.add(element);
			} 
		String[] array = arrayStr.toArray(new String[arrayStr.size()]);
		return array;
	} 
}
